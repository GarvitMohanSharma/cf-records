import java.util.*;
 
public class Alpha {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt(); 
        for (int p = 1; p <= t; p++) {
            int n = sc.nextInt();
            int c = sc.nextInt();
            int d = sc.nextInt();
            int a[]=new int[n*n];
            int ar[][]=new int[n][n];
            for (int i = 0; i < n*n; i++)
            {
                a[i]=sc.nextInt();
            }
            Arrays.sort(a);
            ar[0][0]=a[0];
            for (int i = 1; i < n; i++)
            {
                ar[0][i]=ar[0][i-1]+d;
            }
            for (int i = 1; i < n; i++){
                for (int j = 0; j < n; j++){
                    ar[i][j]=ar[i-1][j]+c;
                }
            }
            int arr[]=new int[n*n];
            int z=0;
            for (int i = 0; i < n; i++){
                for (int j = 0; j < n; j++){
                    arr[z]=ar[i][j];
                    z++;
                }
            }
            Arrays.sort(arr);
            if(Arrays.equals(a, arr))
            {System.out.println("YES");}
            else
            {System.out.println("NO");}
    }
}
}
